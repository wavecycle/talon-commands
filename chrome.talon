app.name: Google Chrome
-

# WhatsApp
chat next: key(ctrl-alt-tab)
chat previous: key(ctrl-alt-shift-tab)

# Gmail
gmail shortcuts: key(shift-/)
compose [email]: key(c)
search [email]: key(/)
select [email]: key(,)
more [options]: key(.)
reply: key(r)
[reply] all: key(a)
inbox:
    key(g)
    key(i)
sent:
    key(g)
    key(t)
drafts:
    key(g)
    key(d)

# Google Docs
docs help:  key(ctrl-/)
apply zero:  key(ctrl-alt-0)
apply one:  key(ctrl-alt-1)